<p id="notice"><%= notice %></p>

<h1>Listing Toyota</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Model</th>
      <th>Acceleration</th>
      <th>Braking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @toyota.each do |toyotum| %>
      <tr>
        <td><%= toyotum.year %></td>
        <td><%= toyotum.model %></td>
        <td><%= toyotum.acceleration %></td>
        <td><%= toyotum.braking %></td>
        <td><%= link_to 'Show', toyotum %></td>
        <td><%= link_to 'Edit', edit_toyotum_path(toyotum) %></td>
        <td><%= link_to 'Destroy', toyotum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Toyotum', new_toyotum_path %>
